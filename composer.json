{
    "name": "assely/framework",
    "type": "wordpress-plugin",
    "description": "Assely is a framework which brings a little joy to the WordPress development. Develop structured, easly scalable websites or web applications with expressive and elegant syntax.",
    "keywords": ["framework", "assely", "wordpress", "php"],
    "license": "MIT",
    "homepage": "http://assely.org",
    "support": {
        "issues": "https://github.com/assely/framework/issues",
        "source": "https://github.com/assely/framework"
    },
    "authors": [
        {
            "name": "Jędrzej Chałubek",
            "email": "jedrzej.chalubek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/http": "^5.3.0",
        "illuminate/container": "^5.3.0",
        "illuminate/support": "^5.3.0",
        "illuminate/contracts": "^5.3.0",
        "illuminate/config": "^5.3.0",
        "illuminate/view": "^5.3.0",
        "illuminate/events": "^5.3.0",
        "illuminate/filesystem": "^5.3.0"
    },
    "require-dev": {
        "brain/monkey": "~1.3.0",
        "mockery/mockery": "~1.0.0",
        "phpunit/phpunit": "~5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Assely\\": "src/Assely"
        },
        "files": [
            "src/Assely/Foundation/helpers.php"
        ]
    },
    "extra": {
        "installer-name": "assely-framework"
    },
    "config": {
        "bin-dir": "bin"
    },
    "suggest": {
        "assely/fielder": "Required to create custom fields."
    },
    "minimum-stability": "dev"
}