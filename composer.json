{
    "name": "assely/framework",
    "type": "wordpress-plugin",
    "description": "Assely is a PHP framework which brings a little joy to the WordPress development. Develop structured, easily scalable and complex WordPress websites and web applications with true pleasure.",
    "keywords": ["framework", "assely", "wordpress", "php"],
    "license": "MIT",
    "homepage": "http://assely.org",
    "support": {
        "issues": "https://github.com/assely/framework/issues",
        "source": "https://github.com/assely/framework"
    },
    "authors": [
        {
            "name": "Jędrzej Chałubek",
            "email": "jedrzej.chalubek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/http": "5.3.*",
        "illuminate/container": "5.3.*",
        "illuminate/support": "5.3.*",
        "illuminate/contracts": "5.3.*",
        "illuminate/config": "5.3.*",
        "illuminate/view": "5.3.*",
        "illuminate/events": "5.3.*",
        "illuminate/filesystem": "5.3.*"
    },
    "require-dev": {
        "brain/monkey": "~1.3.0",
        "mockery/mockery": "~1.0.0",
        "phpunit/phpunit": "~5.3.0",
        "satooshi/php-coveralls": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Assely\\": "src/Assely"
        },
        "files": [
            "src/Assely/Foundation/helpers.php"
        ]
    },
    "extra": {
        "installer-name": "assely-framework"
    },
    "config": {
        "bin-dir": "bin"
    },
    "suggest": {
        "assely/fielder": "Required to create custom fields."
    },
    "minimum-stability": "dev"
}